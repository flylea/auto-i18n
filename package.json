{
  "name": "i18n-a11y",
  "version": "1.0.2",
  "description": "CLI tool for generating i18n files",
  "main": "dist/index.js",
  "bin": {
    "auto-i18n": "dist/src/cli.js"
  },
  "type": "module",
  "scripts": {
    "build": "tsc",
    "i18n:build": "node dist/src/cli.js scan",
    "i18n:local": "tsx src/cli.ts scan",
    "prepublishOnly": "npm run build",
    "patch": "pnpm version patch && npm publish --access public"
  },
  "keywords": [
    "i18n",
    "cli",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/flylea/auto-i18n.git"
  },
  "files": [
    "dist"
  ],
  "author": "hi@flylea.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/flylea/auto-i18n/issues"
  },
  "homepage": "https://github.com/flylea/auto-i18n#readme",
  "devDependencies": {
    "@types/babel__traverse": "^7.28.0",
    "@types/node": "^24.10.2",
    "dotenv": "^17.2.3",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@babel/parser": "^7.28.5",
    "@babel/traverse": "^7.28.5",
    "@babel/types": "^7.28.5",
    "@types/cli-progress": "^3.11.6",
    "@vue/compiler-dom": "^3.5.25",
    "@vue/compiler-sfc": "^3.3.4",
    "chalk": "^4.1.2",
    "cli-progress": "^3.12.0",
    "colors": "^1.4.0",
    "commander": "^14.0.2",
    "fast-glob": "^3.3.3",
    "figlet": "^1.9.4",
    "inquirer": "^13.1.0",
    "openai": "^4.26.0",
    "p-limit": "^5.0.0"
  }
}
